{
    "name": "wldh/voyager",
    "description": "The forked version of WLDH\\Voyager admin panel",
    "keywords": ["laravel", "admin", "panel"],
    "license": "MIT",
    "homepage": "https://w33ladalah.github.io/voyager/",
    "support": {
        "issues": "https://github.com/w33ladalah/voyager/issues",
        "source": "https://github.com/w33ladalah/voyager"
    },
    "authors": [
        {
            "name": "Hendro Wibowo",
            "email": "weeladalah@yahoo.com"
        }
    ],
    "repositories": [
        {
            "url": "https://github.com/w33ladalah/voyager-hooks.git",
            "type": "git"
        }
    ],
    "require": {
        "illuminate/support": "~5.5|~6.0",
        "intervention/image": "^2.4",
        "doctrine/dbal": "^2.5",
        "larapack/doctrine-support": "~0.1.4",
        "arrilot/laravel-widgets": "^3.7",
        "league/flysystem": "~1.0.41",
        "larapack/wldh-voyager-hooks": "^1.2",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpcov": "~4.0|~5.0|~6.0",
        "phpunit/phpunit": "~5.7.14|~6.1|~7.0|~8.0",
        "laravel/framework": "~5.5|~6.0",
        "orchestra/testbench": "~3.0|~4.0",
        "laravel/browser-kit-testing": "~2.0.0|~3.0.0|~4.0.0|~5.0.0|~5.1",
        "orchestra/testbench-browser-kit": "~3.5|~3.6|~3.7|~3.8|~4.0.0"
    },
    "autoload": {
        "psr-4": {
            "WLDH\\Voyager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WLDH\\Voyager\\Tests\\": "tests/"
      }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "WLDH\\Voyager\\VoyagerServiceProvider",
                "WLDH\\Voyager\\Providers\\VoyagerDummyServiceProvider"
            ]
        }
    }
}
